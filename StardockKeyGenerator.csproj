<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <AssemblyName>StardockKeyGenerator</AssemblyName>
    <SelfContained>true</SelfContained>
    <PublishSingleFile>true</PublishSingleFile>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <ApplicationIcon>StardockKeyGenerator.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="StardockKeyGenerator.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="SmorcIRL.TempMail" Version="2.2.0" />
    <EmbeddedResource Include="ClearRegistry.bat" />
  </ItemGroup>
</Project>
